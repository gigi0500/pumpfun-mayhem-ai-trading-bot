# Server Configuration
PORT=3001
NODE_ENV=development

# Solana Configuration
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_PRIVATE_KEY=your_wallet_private_key_here
SOLANA_PUBLIC_KEY=your_wallet_public_key_here

# AI Trading Bot Configuration
AI_ENABLED=true
AI_MODEL_PATH=models/trading_model.pkl
MIN_CONFIDENCE_SCORE=0.7
MAX_POSITION_SIZE=0.2
STOP_LOSS_PERCENTAGE=10
TAKE_PROFIT_PERCENTAGE=50

# Trading Strategy
TRADING_STRATEGY=momentum
SCALPING_ENABLED=false
SWING_TRADING_ENABLED=true

# Risk Management
MAX_DAILY_LOSS=1.0
MAX_OPEN_POSITIONS=5
RISK_PER_TRADE=2

# AI Model Parameters
FEATURE_WINDOW=60
PREDICTION_HORIZON=5
LEARNING_RATE=0.001

# Logging
LOG_LEVEL=info
LOG_FILE=logs/ai-trading.log

